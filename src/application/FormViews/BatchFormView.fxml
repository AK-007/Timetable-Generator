<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8.0.121" xmlns:fx="http://javafx.com/fxml/1" fx:controller="application.FormViews.BatchFormViewController">
    <children>
        <Label fx:id="BatchFormHeading" alignment="CENTER" layoutX="192.0" layoutY="49.0" text="Batch Input Form" textAlignment="CENTER">
            <font>
                <Font size="20.0" />
            </font>
        </Label>
        <TextField fx:id="BatchIdTextField" layoutX="175.0" layoutY="108.0" prefHeight="25.0" prefWidth="269.0" promptText="Batch ID" />
        <TextField fx:id="DepartmentIdTextField" layoutX="175.0" layoutY="174.0" prefHeight="26.0" prefWidth="269.0" promptText="Department ID" />
        <Button fx:id="BatchButtonSubmitForm" layoutX="206.0" layoutY="255.0" mnemonicParsing="false" onAction="#onSubmitAction" prefHeight="26.0" prefWidth="72.0" text="Submit" />
        <Button fx:id="DeleteButtonTextField" layoutX="330.0" layoutY="255.0" mnemonicParsing="false" prefHeight="26.0" prefWidth="72.0" text="Delete" />
    </children>
</AnchorPane>
